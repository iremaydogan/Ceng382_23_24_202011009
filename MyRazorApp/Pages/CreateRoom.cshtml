@page
@model MyApp.Namespace.CreateRoomModel
@{
}
<form method="post" class="form">
    <div class="form-group">
        <label for="RoomName">Room Name:</label>
        <input type="text" id="RoomName" name="rooms.RoomName" class="form-control" required>
    </div>
    <div class="form-group">
        <label for="Capacity">Capacity:</label>
        <input type="number" id="Capacity" name="rooms.Capacity" class="form-control" required>
    </div>
    <button type="submit" class="btn btn-primary">Create Room</button>
        
</form>
@section Scripts {
<partial name="_ValidationScriptsPartial" />
}